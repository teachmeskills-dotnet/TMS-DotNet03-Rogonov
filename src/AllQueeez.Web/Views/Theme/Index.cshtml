@{
}

@model IEnumerable<ThemesViewModel>

@{ 
    ViewData["Title"] = "Themes";
}

@if (Model.Any())
{
    foreach (var theme in Model)
    {
        <div>@theme.Id</div>
        <div>@theme.Name</div>
    }
    <br />
}
else
{
    <div>Empty themes list</div>
}